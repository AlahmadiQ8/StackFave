{
  "name": "StackFavorites",
  "version": "0.0.0",
  "manifest_version": 2,
  "author": "Mohammad Mohammad",
  "description": "List and Search Stackoverflow Favorites",
  "permissions": ["https://stackoverflow.com/*", "storage"],
  "web_accessible_resources": [
    "static/media/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
